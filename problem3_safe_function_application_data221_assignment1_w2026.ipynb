    "def power_of_numbers(x, y):\n",
    "    return x ** y\n",
    "\n",
    "\n",
    "pairs_of_numbers = [[5, 2], [3, -1], [4, 3], [2, 0]]\n",
    "results_after_power = []\n",
    "\n",
    "for x, y in pairs_of_numbers:\n",
    "    if y >= 0:\n",
    "        results_after_power.append(power_of_numbers(x, y))\n",
    "\n",
    "print(results_after_power)"
   ]
  }
